  <body>
<div class="container">
  <div class="panel panel-default" id='1'>
    <div class="panel-body">
      <center><img src="<%= client.user.displayAvatarURL() %>" style="border-radius: 58%; border: 5px solid black;"></center>
      <h3>Name: <b><%= client.user.username %></b></h3>
      <h3>Prefix: <b><%= data.prefix %></b></h3>
      <h3>Guild: <b><%= client.guilds.cache.size %> guilds</b></h3>
      <h3>Users: <b><%= client.guilds.cache.reduce((a, b) => a + b.memberCount, 0).toLocaleString() %> users</b></h3>
      <h3>Memory: <b><%= (process.memoryUsage().heapUsed / 1204 / 1204).toFixed(2) %> MB</b></h3>
    </div>
  </div>
  </div>
    
    <h1>CO-OWNER</h1>
    
    <div class="container">
        <%-
client.guilds.cache.get("713203429744443475").members.cache.filter(m => m._roles.includes("713626430872748063")).map(prop =>`
    
    <div class="panel panel-default">
      <div class="panel-body">
        <center><img src="${client.users.cache.get(prop.user.id).displayAvatarURL()}" style="border: 5px solid black; border-radius: 58%;" class="rounded-circle z-depth-1"
          alt="Sample avatar"></center>
      
      <center><p class="text-uppercase" style="color:${prop.displayHexColor}"><strong>Co-Owner</strong></p></center>
      <h3 class="font-weight-bold mt-4 mb-2">${prop.user.tag}</h3>
      <h3 class="font-weight-bold mt-4 mb-2">${db.get(`decs_${prop.user.id}`) || "Nothing"}</h4>
    </div>
    </div>
    <!-- Grid column -->
`).join(" ")
%>
      </div>
    
    <h1>MODERATOR</h1>
    
    <div class="container">
      <%-
      client.guilds.cache.get("713203429744443475").members.cache.filter(m => m._roles.includes("713494057388605541")).map(prop =>`
      
      <div class="panel panel-default">
      <div class="panel-body">
      <center><img src="${client.users.cache.get(prop.user.id).displayAvatarURL()}" style="border: 5px solid black; border-radius: 58%;" class="rounded-circle z-depth-1"
      alt="sample avatar"></center>
      
      <center><p class="text-uppercase" style="color:${prop.displayHexColor}"><strong>Moderator</strong></p></center>
      <h3 class="font-weight-bold mt-4 mb-2">${prop.user.tag}</h3>
      <h4 class="font-weight-bold mt-4 mb-2">${db.get(`decs_${prop.user.id}`) || "Nothing"}</h4>
      </div>
      </div>
      `).join(" ")
      %>
      </div>

